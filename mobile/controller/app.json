{
  "expo": {
    "name": "controller",
    "slug": "controller",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "controller",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/Idle.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.lisardowo.controller"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/Idle.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/Idle.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "platforms": [
      "ios",
      "android"
    ],
    "assetBundlePatterns": [
      "**/*"
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "4911617a-f03c-4314-a4cf-c7d65d91a89a"
      }
    }
  }
}
